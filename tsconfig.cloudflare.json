{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.base.jsonc",
  "include": [
    ".react-router/types/**/*",
    "app/**/*",
    "app/**/.server/**/*",
    "app/**/.client/**/*",
    "components/**/*",
    "hooks/**/*",
    "lib/**/*",
    "workers/**/*",
    "worker-configuration.d.ts",
    "react-router.config.ts"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.cloudflare.tsbuildinfo",
    "lib": ["DOM", "DOM.Iterable", "ES2023"],
    "types": ["vite/client"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "rootDirs": [".", "./.react-router/types"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "~/*": ["./app/*"],
      "/*": ["./workers/*"]
    },
    "noEmit": false,
    "outDir": "./node_modules/.tmp",
    "declaration": true
  }
}
