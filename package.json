{
  "name": "crrs",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:PRODUCTION": "CLOUDFLARE_ENV=production pnpm build",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm run build && wrangler deploy",
    "deploy:PRODUCTION": "pnpm build:PRODUCTION && wrangler deploy",
    "dev": "react-router dev",
    "postinstall": "npm run cf-typegen",
    "preview": "pnpm run build && vite preview",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "typecheck:test": "tsc -p test/tsconfig.json",
    "lint": "eslint .",
    "test": "vitest run",
    "test:e2e": "pnpm exec playwright test",
    "clean": "pnpm clean:node && pnpm clean:wrangler && pnpm clean:etc",
    "clean:node": "rm -rf node_modules test/*/node_modules",
    "clean:wrangler": "rm -rf .wrangler",
    "clean:etc": "rm -rf .react-router build dist",
    "tail:PRODUCTION": "wrangler tail crrs",
    "d1:migrate:list": "wrangler d1 migrations list d1-local --local",
    "d1:migrate:apply": "wrangler d1 migrations apply d1-local --local",
    "d1:migrate:list:int": "wrangler d1 migrations list crrs-d1-int --env int --remote",
    "d1:migrate:apply:int": "wrangler d1 migrations apply crrs-d1-int --env int --remote",
    "d1:migrate:list:PRODUCTION": "wrangler d1 migrations list crrs-d1-production --env production --remote",
    "d1:migrate:apply:PRODUCTION": "wrangler d1 migrations apply crrs-d1-production --env production --remote",
    "d1:reset": "node scripts/d1-reset.ts",
    "d1:reset:int": "node scripts/d1-reset.ts --env int",
    "d1:reset:PRODUCTION": "node scripts/d1-reset.ts --env production",
    "stripe:listen": "stripe listen --project-name=crrs-int --load-from-webhooks-api --forward-to localhost:5173/",
    "script": "node --env-file=.env"
  },
  "dependencies": {
    "@better-auth/stripe": "1.3.32",
    "@cloudflare/actors": "0.0.1-beta.6",
    "@epic-web/invariant": "1.0.0",
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "aws4fetch": "1.0.20",
    "better-auth": "1.3.32",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "hono": "4.10.4",
    "input-otp": "1.4.2",
    "isbot": "5.1.28",
    "lucide-react": "0.514.0",
    "next-themes": "0.4.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.0",
    "react-aria-components": "1.13.0",
    "react-day-picker": "9.11.1",
    "react-dom": "19.2.0",
    "react-hook-form": "7.65.0",
    "react-resizable-panels": "3.0.6",
    "react-router": "7.9.4",
    "recharts": "2.15.4",
    "remix-themes": "2.0.4",
    "simple-icons": "15.16.1",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "vaul": "1.1.2",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@better-auth/cli": "1.3.32",
    "@cloudflare/vite-plugin": "1.13.3",
    "@cloudflare/vitest-pool-workers": "0.9.3",
    "@eslint/js": "9.38.0",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@playwright/test": "1.56.1",
    "@react-router/dev": "7.9.4",
    "@react-router/fs-routes": "7.9.4",
    "@tailwindcss/vite": "4.1.16",
    "@types/node": "22.13.14",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.38.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "jsonc-parser": "3.3.1",
    "prettier": "3.6.2",
    "prettier-plugin-sql": "0.19.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "shadcn": "3.4.2",
    "stripe": "19.3.0",
    "tailwindcss": "4.1.16",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.46.2",
    "vite": "7.1.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "wrangler": "4.38.0",
    "zx": "8.5.5"
  },
  "packageManager": "pnpm@10.4.0",
  "engines": {
    "node": "22.18.0"
  },
  "volta": {
    "node": "22.18.0",
    "pnpm": "10.4.0"
  }
}
